{
    "name": "tunga-web",
    "scripts": {
        "clean": "del-cli --dot=true \"./build/**/*\"",
        "build:web": "cross-env NODE_ENV=production webpack --mode production --config webpack.production.config.js",
        "build:web:sandbox": "cross-env NODE_ENV=production STRIPE_ENV=development ANALYTICS_ENV=development webpack --mode production --config webpack.production.config.js",
        "build:web:dev": "cross-env API_ROOT=http://localhost:8000/ SSR=1 webpack --mode development",
        "build:server": "cross-env NODE_ENV=production webpack --mode production --config webpack.server.config.js",
        "build:server:dev": "cross-env webpack --mode development --config webpack.server.config.js",
        "build": "run-s clean build:web build:server",
        "build:sandbox": "run-s clean build:web:sandbox build:server",
        "build:dev": "run-s clean build:web:dev build:server:dev",
        "build:dev:sandbox": "cross-env API_ROOT=https://sandbox.tunga.io/ run-s clean build:web:sandbox build:server:dev",
        "watch": "cross-env webpack-dev-server --hot --mode development",
        "watch:dev": "cross-env API_ROOT=http://localhost:8000/ webpack-dev-server --hot --mode development",
        "watch:server": "webpack --watch --mode development --config webpack.server.config.js",
        "watch:web": "webpack --watch --mode development",
        "watch:web:dev": "cross-env API_ROOT=http://localhost:8000/ webpack --watch --mode development",
        "monitor:build": "nodemon",
        "watch:ssr": "run-p watch:web watch:server monitor:build",
        "watch:ssr:dev": "run-p watch:web:dev watch:server monitor:build",
        "server": "node ./build/server.js",
        "server:dev": "run-s build:dev server",
        "server:dev:sandbox": "run-s build:dev:sandbox server",
        "lint": "node ./scripts/tasks/eslint.js",
        "prettier": "node ./scripts/prettier/index.js write-changed",
        "prettier:all": "node ./scripts/prettier/index.js write",
        "deploy": "cd .ansible && ansible-playbook deploy.yml -i env/prod && cd ..",
        "deploy:sandbox": "cd .ansible && ansible-playbook deploy.yml -i env/sandbox && cd ..",
        "test": "echo \"No unit tests and e2e tests yet\" && exit 0"
    },
    "version": "0.1.0",
    "license": "MIT",

{
    "restartable": "rs",
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "ignore": [
        "node_modules",
        "src",
        "scripts",
        ".ansible",
        ".github"
    ],
    "watch": [
        "build"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js jsx json less scss",
    "exec": "npm run server"
}
